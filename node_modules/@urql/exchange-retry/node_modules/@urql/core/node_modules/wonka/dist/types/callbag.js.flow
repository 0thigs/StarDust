// @flow

import type { Source } from "./types";
declare interface Callbag<I, O> {
  (t: 0, d: Callbag<O, I>): void;
  (t: 1, d: I): void;
  (t: 2, d?: any): void;
}
declare export function fromCallbag<T>(callbag: Callbag<any, T>): Source<T>;
declare export function toCallbag<T>(source: Source<T>): Callbag<any, T>;

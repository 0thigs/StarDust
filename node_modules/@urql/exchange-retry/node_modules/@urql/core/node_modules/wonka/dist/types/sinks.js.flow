// @flow

import type { Source, Subscription } from "./types";
declare export function subscribe<T>(
  subscriber: (value: T) => void
): (source: Source<T>) => Subscription;
declare export function forEach<T>(
  subscriber: (value: T) => void
): (source: Source<T>) => void;
declare export function publish<T>(source: Source<T>): void;
declare export function toArray<T>(source: Source<T>): T[];
declare export function toPromise<T>(source: Source<T>): Promise<T>;
